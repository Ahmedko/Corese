st:dbpedia a st:Profile ;
  st:query     "frdbpedia.rq" ;
  st:variable  "?r" ;
  st:transform st:navlab .

st:dbpediacopy a st:Profile ;
  st:query     "frdbpediains.rq" ;
  st:variable  "?r" ;
  st:transform st:navlab .

st:dbpediaall a st:Profile ;
  st:query     "frdbpediaall.rq" ;
  st:variable  "?r" ;
  st:transform st:navlab .


st:navlab a st:Profile ;
  st:variable "?r" ;  
  st:transform st:navlab .

st:web a st:Profile ;
   st:transform st:web .

st:weben a st:Profile ;
   st:lang 'en' ;
   st:transform st:web .

st:sparql a st:Profile ;
    st:query "construct.rq" ;
    st:variable "?r" ;
    st:transform st:sparql .

st:std a st:Profile ;
    st:query "construct.rq" ;
    st:variable "?r" ;
    st:transform st:sparql .

st:local a st:Profile ;
  st:query     "local.rq" ;
  st:variable  "?r" ;
  st:transform st:navlab .

st:personen a st:Profile ;
  st:query     "dbpedia.rq" ;
  st:variable  "?r" ;
  st:transform st:navlab .  
  
  
st:bound a st:Profile ;
st:query "bound.rq";
st:transform st:navlab .  
 
st:mglt a st:Profile ;
st:query "mglt.rq";
st:transform st:navlab . 

st:spqr a st:Profile ;
st:query "spqr.rq";
st:transform st:navlab . 
       
st:cdn a st:Profile ;
st:query "cdn.rq";
st:transform st:navlab .  

