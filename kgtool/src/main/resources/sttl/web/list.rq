#
# Generate query names list from context graph
#
template st:list(?t) {

format {
  """
  <option value='%s'" %s> 
  %s . %s 
  </option>
  """
  
  ?q
  if (?q = ?t, "selected='true'", "")
  str(?i)
  str(?title)
}
   
}
where {
  bind (st:get(st:context) as ?g)
  graph ?g {
    [] a st:Query ;
      st:index ?i ;
      st:name ?q ;
      rdfs:label ?title
  }
}
order by ?i
