#
# Get list st:topic from profile.ttl st:param
#
prefix cn: <http://cdn.fr/schema/>
template cn:row(?d) {

  format {
    "<td>%s</td>"
    
    group {
	st:call-template(cn:date, ?d, ?topic)
    } 
  }
  

}
where {
  bind (st:get(st:definition) as ?def)
  bind (unnest(st:get(st:topic)) as ?e)
  graph ?def {
    ?e rdf:rest*/rdf:first ?topic
  }
  
}
group by ?e
         